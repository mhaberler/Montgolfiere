{
  "name": "Montgolfiere",
  "private": true,
  "version": "1.5.9",
  "type": "module",
  "scripts": {
    "watch": "vue-tsc --watch",
    "dev": "vite --port=8100 --host",
    "clean": "bunx rimraf ./dist",
    "bump-bundle": "bunx npm version patch --no-git-tag-version && fastlane bump",
    "capgo-upload-bundle": "vue-tsc && vite build --mode development --outDir ./dist.capgo.development && bunx @capgo/cli@latest bundle upload --path ./dist.capgo.development --channel=development",
    "bump-minor": "bunx npm version minor --no-git-tag-version && fastlane bump",
    "bump-major": "bunx npm version major --no-git-tag-version && fastlane bump",
    "build": "vue-tsc && vite build --mode development",
    "preview": "vite preview",
    "prepare-beta": "npm run clean && npm run build && npm run sync",
    "ios-beta": "fastlane ios beta",
    "android-beta": "fastlane android beta",
    "deploy-mah.priv.at": "vite build --mode development --outDir ./dist.webtest --base=/apps/montgolfiere/  && rsync -avz --delete ./dist.webtest/ mah@mah.priv.at:/var/www/static.mah.priv.at/apps/montgolfiere/ && qrcode-terminal https://static.mah.priv.at/apps/montgolfiere/",
    "debug-android": "bunx cap run android  --live-reload --host 172.16.0.212 --port 8100  --target R3CX90A30EW",
    "debug-ios": "export PATH=\"/opt/homebrew/opt/ruby/bin:$PATH\" &&  bunx cap run ios --live-reload --host  172.16.0.212 --port 8100  --target 00008140-0002692C1E79801C",
    "run-on-galaxy-s24": "bunx cap run android --target R3CX90A30EW",
    "run-on-galaxy-s10": "bunx cap run android --target RF8MC11LGDW",
    "run-on-galaxy-pro-tab10": "bunx cap run android --target R52T40AW12D",
    "run-on-iphone": "bunx cap run ios --target 00008140-0002692C1E79801C",
    "run-on-ipad": "bunx cap run ios --target 7469941401580df80750b83afac3dec9bde0309a",
    "capgo-bundles": "bunx @capgo/cli@latest bundle list",
    "capgo-channels": "bunx @capgo/cli@latest channel list",
    "capgo-create-app": "bunx @capgo/cli@latest app add",
    "capgo-delete-app": "bunx @capgo/cli@latest app delete",
    "capgo-list-apps": "bunx @capgo/cli@latest app list",
    "capgo-debug-app": "bunx @capgo/cli@latest app debug",
    "capgo-clean": "bunx rimraf  ./dist.capgo.development",
    "ios-log": "idevicesyslog",
    "logcat:all": "adb logcat",
    "logcat:clear": "adb logcat -c",
    "logcat:app": "adb logcat -v threadtime Capacitor:D 'Capacitor/Console':D *:S",
    "logcat:app:device": "adb -d logcat -v threadtime Capacitor:D 'Capacitor/Console':D *:S",
    "logcat:app:emulator": "adb -e logcat -v threadtime Capacitor:D 'Capacitor/Console':D *:S",
    "logcat:app:mydevice": "adb -s RF8MC11LGDW logcat -v threadtime Capacitor:D 'Capacitor/Console':D *:S",
    "logcat:app:output": "adb logcat -d -v threadtime Capacitor:D 'Capacitor/Console':D *:S > android_logs.txt",
    "open-in-Xcode": "bunx cap open ios",
    "open-in-Android-Studio": "bunx cap open android",
    "sync": "export PATH=\"/opt/homebrew/opt/ruby/bin:$PATH\" && bunx cap sync"
  },
  "scripts-disabled": {
    "deploy-nocache-mah.priv.at": "vite build --mode development --base=/apps/montgolfiere/  && rsync -avz --delete dist/ mah@mah.priv.at:/var/www/static.mah.priv.at/apps-nocache/montgolfiere/ && qrcode-terminal https://static.mah.priv.at/apps-nocache/montgolfiere/?simulateBarometer=true",
    "run-on-ios": "bunx cap run ios",
    "test:e2e": "bunx cypress run",
    "test:unit": "bunx vitest",
    "lint": "bunx eslint .",
    "capgo-production-upload": "vue-tsc && vite build --mode production --outDir  ./dist.capgo.production   && bunx @capgo/cli@latest bundle upload --path ./dist.capgo.production --channel=production",
    "production": "vite --mode production --host 172.16.0.212 --port=8100 --host",
    "debug-android-no-mirror": "npx cap run android  --live-reload --host 172.16.0.212 --port 8100  --target R3CX90A30EW",
    "dev-external": "vite --host 172.16.0.212 --port=8100",
    "preview": "vite --host 172.16.0.212  preview",
    "predeploy": "npm run build",
    "cmdline-ios-build": "npx cap build ios",
    "cmdline-android-build": "npx cap build android",
    "build-production": "vue-tsc && vite build --mode production",
    "preview-host": "vite --host 172.16.0.212 --port 4712 preview",
    "///capacitor-barometer": "file:../capacitor-barometer"
  },
  "dependencies": {
    "@capacitor-community/apple-sign-in": "7.0.1",
    "@capacitor-community/barcode-scanner": "^4.0.1",
    "@capacitor-community/bluetooth-le": "^7.1.1",
    "@capacitor-community/camera-preview": "7.0.1",
    "@capacitor-community/keep-awake": "^7.1.0",
    "@capacitor-community/safe-area": "^7.0.0-alpha.1",
    "@capacitor/action-sheet": "^7.0.1",
    "@capacitor/android": "7.4.0",
    "@capacitor/app": "^7.0.1",
    "@capacitor/app-launcher": "^7.0.1",
    "@capacitor/barcode-scanner": "2.0.3",
    "@capacitor/browser": "^7.0.1",
    "@capacitor/camera": "^7.0.1",
    "@capacitor/clipboard": "^7.0.1",
    "@capacitor/core": "7.4.0",
    "@capacitor/device": "^7.0.1",
    "@capacitor/dialog": "^7.0.1",
    "@capacitor/filesystem": "^7.1.1",
    "@capacitor/geolocation": "^7.1.2",
    "@capacitor/haptics": "7.0.1",
    "@capacitor/inappbrowser": "2.0.1",
    "@capacitor/ios": "7.4.0",
    "@capacitor/keyboard": "7.0.1",
    "@capacitor/local-notifications": "^7.0.1",
    "@capacitor/motion": "^7.0.0",
    "@capacitor/network": "^7.0.1",
    "@capacitor/preferences": "^7.0.1",
    "@capacitor/screen-orientation": "^7.0.1",
    "@capacitor/screen-reader": "^7.0.1",
    "@capacitor/share": "^7.0.1",
    "@capacitor/splash-screen": "^7.0.1",
    "@capacitor/status-bar": "7.0.1",
    "@capacitor/text-zoom": "^7.0.1",
    "@capacitor/toast": "^7.0.1",
    "@capawesome/capacitor-background-task": "^7.0.1",
    "@capawesome/capacitor-file-picker": "^7.2.0",
    "@capawesome/capacitor-screen-orientation": "^7.0.1",
    "@capawesome/capacitor-screenshot": "^7.0.1",
    "@capgo/capacitor-updater": "^7.6.0",
    "@capgo/native-audio": "^7.3.34",
    "@ionic/prettier-config": "^4.0.0",
    "@ionic/remote-log": "^0.0.2",
    "@ionic/vue": "^8.6.2",
    "@ionic/vue-router": "^8.6.2",
    "@sentry/capacitor": "^1.4.0",
    "@sentry/vue": "8.55.0",
    "canvas-gauges": "^2.1.7",
    "capacitor-barometer": "file:../capacitor-barometer",
    "capacitor-set-version": "^2.2.0",
    "dotenv": "^16.6.1",
    "html2canvas": "^1.4.1",
    "ionicons": "^7.4.0",
    "mathjs": "^14.5.2",
    "meteojs": "^1.17.3",
    "mqtt": "^5.13.1",
    "qrcode-terminal": "^0.12.0",
    "rimraf": "^6.0.1",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@capacitor/cli": "7.4.0",
    "@vitejs/plugin-legacy": "^5.4.3",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.6",
    "cypress": "^13.17.0",
    "eslint": "^8.57.1",
    "eslint-plugin-vue": "^9.33.0",
    "jsdom": "^22.1.0",
    "terser": "5.43.1",
    "typescript": "5.8.3",
    "vite": "5.4.19",
    "vite-plugin-devtools-json": "^0.2.1",
    "vite-plugin-qrcode": "^0.2.4",
    "vitest": "^0.34.6",
    "vue-tsc": "^2.2.10"
  },
  "description": "An Ionic project"
}
