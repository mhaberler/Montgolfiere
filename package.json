{
  "name": "Montgolfiere",
  "private": true,
  "version": "1.2.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview-orig": "vite --host 172.16.0.212  preview",
    "preview": "vite --host 172.16.0.212 --port 4712 preview",
    "test:e2e": "cypress run",
    "test:unit": "vitest",
    "lint": "eslint .",
    "predeploy": "npm run build",
    "sync": "npx cap sync",
    "debug-android": "ionic capacitor run android --livereload --external --public-host 172.16.0.212 --target R3CX90A30EW",
    "open-in-Xcode": "npx cap open ios",
    "open-in-Android-Studio": "npx cap open android",
    "cmdline-ios-build": "npx cap build ios",
    "cmdline-android-build": "npx cap build android",
    "run-on-galaxy-s24": "npx cap run android --target R3CX90A30EW",
    "run-on-galaxy-s10": "npx cap run android --target RF8MC11LGDW",
    "run-on-galaxy-pro-tab10": "npx cap run android --target R52T40AW12D",
    "run-on-iphone": "npx cap run ios --target 00008140-0002692C1E79801C",
    "run-on-ipad": "npx cap run ios --target 7469941401580df80750b83afac3dec9bde0309a",
    "run-on-ios": "npx cap run ios",
    "ios-log": "idevicesyslog",
    "logcat:all": "adb logcat",
    "logcat:clear": "adb logcat -c",
    "logcat:app": "adb logcat -v threadtime Capacitor:D 'Capacitor/Console':D *:S",
    "logcat:app:device": "adb -d logcat -v threadtime Capacitor:D 'Capacitor/Console':D *:S",
    "logcat:app:emulator": "adb -e logcat -v threadtime Capacitor:D 'Capacitor/Console':D *:S",
    "logcat:app:mydevice": "adb -s RF8MC11LGDW logcat -v threadtime Capacitor:D 'Capacitor/Console':D *:S",
    "logcat:app:output": "adb logcat -d -v threadtime Capacitor:D 'Capacitor/Console':D *:S > android_logs.txt",
    "bump-patch": "npm version patch --no-git-tag-version && fastlane bump",
    "bump-minor": "npm version minor --no-git-tag-version && fastlane bump",
    "bump-major": "npm version major --no-git-tag-version && fastlane bump",
    "ios-beta": "fastlane ios beta",
    "android-beta": "fastlane android beta",
    "deploy-hosted": "vite build  --base=/apps/montgolfiere/  && rsync -avz --delete dist/ mah@mah.priv.at:/var/www/static.mah.priv.at/apps/montgolfiere/",
    "qrcode-mah.priv.at": "qrcode-terminal https://static.mah.priv.at/apps/montgolfiere/"
  },
  "dependencies": {
    "@capacitor-community/bluetooth-le": "^7.1.1",
    "@capacitor-community/keep-awake": "^7.1.0",
    "@capacitor/android": "7.4.0",
    "@capacitor/app": "7.0.1",
    "@capacitor/core": "7.4.0",
    "@capacitor/geolocation": "^7.1.2",
    "@capacitor/haptics": "7.0.1",
    "@capacitor/ios": "7.4.0",
    "@capacitor/keyboard": "7.0.1",
    "@capacitor/preferences": "^7.0.1",
    "@capacitor/status-bar": "7.0.1",
    "@capacitor/toast": "^7.0.1",
    "@ionic/vue": "^8.0.0",
    "@ionic/vue-router": "^8.0.0",
    "canvas-gauges": "^2.1.7",
    "capacitor-barometer": "file:../capacitor-barometer",
    "dotenv": "^16.5.0",
    "ionicons": "^7.0.0",
    "mathjs": "^14.5.2",
    "meteojs": "^1.17.3",
    "qrcode-terminal": "^0.12.0",
    "vue": "^3.3.0",
    "vue-router": "^4.2.0"
  },
  "devDependencies": {
    "@capacitor/cli": "7.4.0",
    "@vitejs/plugin-legacy": "^5.0.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.3.0",
    "cypress": "^13.5.0",
    "eslint": "^8.35.0",
    "eslint-plugin-vue": "^9.9.0",
    "jsdom": "^22.1.0",
    "terser": "5.43.1",
    "typescript": "5.8.3",
    "vite": "5.4.19",
    "vite-plugin-devtools-json": "^0.2.0",
    "vite-plugin-qrcode": "^0.2.4",
    "vitest": "^0.34.6",
    "vue-tsc": "^2.1.10"
  },
  "description": "An Ionic project"
}
