    "@capgo/nativegeocoder": "^7.1.7",
    "@ionic/prettier-config": "^4.0.0",
    "@ionic/remote-log": "^0.0.2",
    "@ionic/vue": "^8.6.4",
    "@ionic/vue-router": "^8.6.4",
    "@mhaberler/capacitor-barometer": "^1.1.4",
    "@tailwindcss/vite": "^4.1.11",
    "@types/yup": "^0.32.0",
    "canvas-gauges": "^2.1.7",
    "capacitor-set-version": "^2.2.0",
    "core-js": "^3.44.0",
    "dotenv": "^17.2.0",
    "html2canvas": "^1.4.1",
    "install": "^0.13.0",
    "ionicons": "^8.0.13",
