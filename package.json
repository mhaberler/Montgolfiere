{
  "name": "Montgolfiere",
  "private": true,
  "version": "1.7.0",
  "type": "module",
  "description": "An Ionic project",
  "config": {
    "host": "172.16.0.212",
    "port": "8100"
  },
  "scripts": {
    "watch": "vue-tsc --watch",
    "dev": "vite --port=$npm_package_config_port --host",
    "clean": "bunx rimraf ./dist",
    "current-bundle-id": "jq -r '.bundleversion' bundle.json",
    "capgo-upload-bundle": "npm run bump-bundle-id && vue-tsc && vite build --mode development --outDir ./dist.capgo.development && bunx @capgo/cli@latest bundle upload -b $(jq -r '.bundleversion' bundle.json) --path ./dist.capgo.development --channel=development",
    "build": "vue-tsc && vite build --mode development",
    "preview": "vite preview",
    "prepare-beta": "npm run clean && npm run build && npm run sync",
    "ios-beta": "fastlane ios beta",
    "android-beta": "fastlane android beta",
    "deploy-mah.priv.at": "vite build --mode development --outDir ./dist.webtest --base=/apps/montgolfiere/  && rsync -avz --delete ./dist.webtest/ mah@mah.priv.at:/var/www/static.mah.priv.at/apps/montgolfiere/ && echo  https://static.mah.priv.at/apps/montgolfiere/ && qrcode-terminal https://static.mah.priv.at/apps/montgolfiere/",
    "debug-android-s24": "bunx cap run android  --live-reload --host $npm_package_config_host --port $npm_package_config_port  --target R3CX90A30EW",
    "debug-android-s10": "bunx cap run android  --live-reload --host $npm_package_config_host --port $npm_package_config_port  --target RF8MC11LGDW",
    "debug-android-a15": "bunx cap run android  --live-reload --host $npm_package_config_host --port $npm_package_config_port  --target RF8Y109W1JB",
    "debug-ios": "export PATH=\"/opt/homebrew/opt/ruby/bin:$PATH\" &&  bunx cap run ios --live-reload --host  $npm_package_config_host --port $npm_package_config_port  --target 00008140-0002692C1E79801C",
    "run-on-galaxy-s24": "bunx cap run android --target R3CX90A30EW",
    "run-on-galaxy-s10": "bunx cap run android --target RF8MC11LGDW",
    "run-on-galaxy-a15": "bunx cap run android --target RF8Y109W1JB",
    "run-on-galaxy-pro-tab10": "bunx cap run android --target R52T40AW12D",
    "run-on-iphone": "bunx cap run ios --target 00008140-0002692C1E79801C",
    "run-on-ipad": "bunx cap run ios --target 7469941401580df80750b83afac3dec9bde0309a",
    "capgo-bundles": "bunx @capgo/cli@latest bundle list",
    "capgo-channels": "bunx @capgo/cli@latest channel list",
    "capgo-create-app": "bunx @capgo/cli@latest app add",
    "capgo-delete-app": "bunx @capgo/cli@latest app delete",
    "capgo-list-apps": "bunx @capgo/cli@latest app list",
    "capgo-debug-app": "bunx @capgo/cli@latest app debug",
    "capgo-clean": "bunx rimraf  ./dist.capgo.development",
    "ios-log": "idevicesyslog",
    "logcat:all": "adb logcat",
    "logcat:clear": "adb logcat -c",
    "logcat:app": "adb logcat -v threadtime Capacitor:D 'Capacitor/Console':D *:S",
    "logcat:app:device": "adb -d logcat -v threadtime Capacitor:D 'Capacitor/Console':D *:S",
    "logcat:app:emulator": "adb -e logcat -v threadtime Capacitor:D 'Capacitor/Console':D *:S",
    "logcat:app:mydevice": "adb -s RF8MC11LGDW logcat -v threadtime Capacitor:D 'Capacitor/Console':D *:S",
    "logcat:app:output": "adb logcat -d -v threadtime Capacitor:D 'Capacitor/Console':D *:S > android_logs.txt",
    "open-in-Xcode": "bunx cap open ios",
    "open-in-Android-Studio": "bunx cap open android",
    "bump-bundle-id": "node scripts/bump-capgo-bundle-version.js",
    "capgo-zip-bundle": "vue-tsc && vite build --mode development --outDir ./dist.capgo.development && bunx @capgo/cli@latest bundle zip -b $(jq -r '.bundleversion' bundle.json) --path ./dist.capgo.development",
    "app-bump-patch": "bunx npm version patch --no-git-tag-version && fastlane bump",
    "app-bump-minor": "bunx npm version minor --no-git-tag-version && fastlane bump",
    "app-bump-major": "bunx npm version major --no-git-tag-version && fastlane bump",
    "sync": "export PATH=\"/opt/homebrew/opt/ruby/bin:$PATH\" && bunx cap sync",
    "env": "echo  $npm_package_config_host  $npm_package_config_port"
  },
  "scripts-disabled": {
    "test": "cross-var echo bundle version $npm_package_bundle $npm_package_name v${npm_package_version}",
    "env": "echo cb $npm_package_config_bundle  b $npm_package_bundle",
    "ios-build-number": "cd ios/App && agvtool what-version ",
    "bundle-version": "cd ios/App && agvtool what-version ",
    "environ": "printenv",
    "zipitb": "bunx @capgo/cli@latest bundle zip com.haberler.montgolfiere --path ./dist.capgo.development -b $npm_package_config_bundle ",
    "run-on-ios": "bunx cap run ios",
    "test:e2e": "bunx cypress run",
    "test:unit": "bunx vitest",
    "lint": "bunx eslint .",
    "capgo-production-upload": "vue-tsc && vite build --mode production --outDir  ./dist.capgo.production   && bunx @capgo/cli@latest bundle upload --path ./dist.capgo.production --channel=production",
    "production": "vite --mode production --host $npm_package_config_host --port=$npm_package_config_port --host",
    "debug-android-no-mirror": "npx cap run android  --live-reload --host $npm_package_config_host --port $npm_package_config_port  --target R3CX90A30EW",
    "dev-external": "vite --host $npm_package_config_host --port=$npm_package_config_port",
    "preview": "vite --host $npm_package_config_host  preview",
    "predeploy": "npm run build",
    "cmdline-ios-build": "npx cap build ios",
    "cmdline-android-build": "npx cap build android",
    "build-production": "vue-tsc && vite build --mode production",
    "preview-host": "vite --host $npm_package_config_host --port 4712 preview"
  },
  "dependencies": {
    "@capacitor-community/apple-sign-in": "7.0.1",
    "@capacitor-community/barcode-scanner": "^4.0.1",
    "@capacitor-community/bluetooth-le": "^7.1.1",
    "@capacitor-community/camera-preview": "7.0.2",
    "@capacitor-community/keep-awake": "^7.1.0",
    "@capacitor-community/safe-area": "^7.0.0-alpha.1",
    "@capacitor/action-sheet": "^7.0.1",
    "@capacitor/android": "7.4.2",
    "@capacitor/app": "^7.0.1",
    "@capacitor/app-launcher": "^7.0.1",
    "@capacitor/barcode-scanner": "2.0.3",
    "@capacitor/browser": "^7.0.1",
